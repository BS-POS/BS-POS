@startuml
!define DARKBLUE
!include https://github.com/Drakemor/RedDress-PlantUML.git



Class CustomerCheck {
ID
ServerName
CheckName
TimeStamp
TableNumber
CheckTotal
PaymentMethod
LastFourCardDigits
ItemLog
Getters()
Setters(Object objectName))
}

Class Item {
ID
ItemName
Price
ItemCount
InStock
Getters()
Setters(Object objectName)
}

Class Modifier {
ID
ModifierName
Price
Quantity
InStock
Getters()
Setters(Object objectName))
}

note "ArrayList 'ItemLog' collects number of total items on CustomerCheck" as N1
CustomerCheck ..N1
N1.. Item

note "ArrayList 'ItemCount' collects number of specific item" as N2
Item ..N2
N2.. Modifier

@enduml